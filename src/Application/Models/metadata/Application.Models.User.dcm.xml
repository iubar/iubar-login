<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Models\User" table="User">
    <id name="username" type="string" column="Username" length="255">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="email" type="string" column="Email" length="64" nullable="true"/>
    <field name="lastlogin" type="datetime" column="LastLogin" nullable="true"/>
    <field name="active" type="boolean" column="Active" nullable="true"/>
    <field name="activationhash" type="string" column="ActivationHash" length="40" nullable="true"/>
    <field name="remembermetoken" type="string" column="RememberMeToken" length="64" nullable="true"/>
    <field name="pwdhash" type="string" column="PwdHash" length="255" nullable="true"/>
    <field name="pwdresethash" type="string" column="PwdResetHash" length="40" nullable="true"/>
    <field name="pwdresettime" type="datetime" column="PwdResetTime" nullable="true"/>
    <field name="failedlogins" type="boolean" column="FailedLogins" nullable="true"/>
    <field name="lastfailedlogin" type="datetime" column="LastFailedLogin" nullable="true"/>
    <field name="creationtime" type="datetime" column="CreationTime" nullable="true"/>
    <field name="creationip" type="string" column="CreationIp" length="39" nullable="true"/>
    <field name="sessionid" type="string" column="SessionId" length="48" nullable="true"/>
    <field name="deleted" type="boolean" column="Deleted" nullable="true"/>
    <field name="accounttype" type="boolean" column="AccountType" nullable="true"/>
    <field name="hasavatar" type="boolean" column="HasAvatar" nullable="true"/>
    <field name="providertype" type="string" column="ProviderType" length="10" nullable="true"/>
    <field name="suspensiontype" type="datetime" column="SuspensionType" nullable="true"/>
    <field name="apikey" type="string" column="ApiKey" length="25" nullable="true"/>
    <field name="suspensiontime" type="datetime" column="SuspensionTime" nullable="true"/>
  </entity>
</doctrine-mapping>
